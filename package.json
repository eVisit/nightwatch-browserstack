  },
  "repository": {
    "type": "git",
    "url": "https://github.com/browserstack/nightwatch-browserstack"
  },
  "dependencies": {
    "browserstack-local": "^1.4.5",
    "chromedriver": "^91.0.1",
    "nightwatch": "^1.6.4",
    "nightwatch-browserstack": "^1.3.0",
    "node-random-name": "^1.0.1",
    "request": "^2.88.2",
    "selenium-webdriver": "^4.0.0-alpha.7"
  },
  "license": "MIT",
  "devDependencies": {
    "geckodriver": "^1.22.2"
  }
